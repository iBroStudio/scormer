{
    "name": "ibrostudio/scormer",
    "description": "Generate a scorm package from a static html/js site",
    "keywords": ["scorm", "scorm generator"],
    "homepage": "https://github.com/iBroStudio/scormer",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/iBroStudio/scormer/issues",
        "source": "https://github.com/iBroStudio/scormer"
    },
    "authors": [
        {
            "name": "iBroStudio",
            "email": "hello@ibro.studio"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/termwind": "^1.15.1",
        "spatie/laravel-data": "^3.5",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "laravel-zero/framework": "^10.0.2",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/scormer"]
}
